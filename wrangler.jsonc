{
   "$schema": "node_modules/wrangler/config-schema.json",
   "name": "barroit-sh",
   "main": "./build/worker.js",
   "compatibility_date": "2025-09-27",
   "assets": {
      "directory": "build/static",
      "html_handling": "drop-trailing-slash",
      "run_worker_first": [ "/q/*" ],
   },
   "observability": {
      "enabled": true,
   },
   "routes": [
      {
         "pattern": "barroit.sh",
         "custom_domain": true,
      },
   ],
}
